        <name>x_wec_solara_admin.team.focus-drctv</name>
        <script><![CDATA[(function () {
    'use strict';

    angular
        .module('twc-product')
        .directive('focus', FocusDirective);

	function FocusDirective() {
		return {
			restrict: 'A',
			controller: FocusController,
			scope: {
				focus: '@'
			}
		};
	}
 	
	FocusController.$inject = ['$scope', '$element'];
	
	function FocusController($scope, $element) {
		$scope.focus === 'true' && $element[0].focus();
	}

})();

]]></script>
